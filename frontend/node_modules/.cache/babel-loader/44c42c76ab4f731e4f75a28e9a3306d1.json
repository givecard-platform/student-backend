\">\n          \n            <td>{{ user.firstName }}</td>\n            <td>{{ user.lastName }}</td>\n            <td>{{ user.email }}</td>\n            <td>{{ user.status }}</td>\n            <td>\n              <button class=\"btn btn-warning\" \n              v-on:click=\"updateUser(user.id)\">\n                Update\n              </button>\n            </td>\n            <td>\n              <button class=\"btn btn-danger\" \n              v-on:click=\"deleteUser(user.id)\">\n                Delete\n              </button>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n      <div class=\"row\">\n        <button class=\"btn btn-success\" \n        v-on:click=\"addUser(uuid)\">Add</button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport UserDataService from \"../service/UserDataService\";\nimport { uuid } from 'vue-uuid';\n\nexport default {\n  name: \"Users\",\n  data() {\n    return {\n      uuid: uuid.v1(),\n      users: [],\n      message: \"\",\n    };\n  },\n  methods: {\n    refreshUsers() {\n      UserDataService.retrieveAllUsers().then((res) => {\n        this.users = res.data;\n      });\n    },\n    addUser(uuid) {\n      this.$router.push(`/user/${uuid}`);\n    },\n    updateUser(id) {\n      this.$router.push(`/user/${id}`);\n    },\n    deleteUser(id) {\n      UserDataService.deleteUser(id).then(() => {\n        this.refreshUsers();\n      });\n    },\n  },\n  created() {\n    this.refreshUsers();\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}